<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="title">
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="dark">
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="TextViewStyle">
        <item name="textAllCaps">false</item>
    </style>
    <style name="ButtonMainStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textAppearance">@style/title</item>
        <item name="android:textColor">@color/black</item>
        <item name="cornerRadius">@dimen/dp_16</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/dp_8</item>
        <item name="iconTint">@color/black</item>
    </style>
    <style name="TextViewSettingStyle">
        <item name="android:textAppearance">@style/dark</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="textAllCaps">false</item>
    </style>
    <style name="ImageButtonBack">
        <item name="android:src">@drawable/ic_back</item>
    </style>
</resources>